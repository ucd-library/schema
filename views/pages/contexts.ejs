<%- include('../partials/head') %>
<%- include('../partials/header') %>
<main class="page-content" aria-label="Content">
    <div class="l-basic--3col" id="blog">

        <div class="l-sidebar-first panel o-box"></div>
        <div class="l-content panel o-box">
            <h4 style="font-size:30px">Contexts:</h4>
                <% for (post of posts) { %>
                    <a  style="font-size:22px" href="<%= post.slice(0, -3) %>">
                    <div class="post">
                        <%= post %>  
                    </div>
                    </a> 
            <% } %>
        </div>
        <div class="l-sidebar-second panel o-box"></div> 
    </div>
</main>
<%- include('../partials/footer') %>